<?xml version="1.0" encoding="utf-8"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="urn:oasis:names:tc:xliff:document:1.2 xliff-core-1.2-transitional.xsd">
  <file datatype="xml" source-language="en-US" target-language="fa-IR" original="PasswordReset.ascx.resx" tool-id="MultilingualAppToolkit" product-name="n/a" product-version="n/a" build-num="n/a">
    <header>
      <tool tool-id="MultilingualAppToolkit" tool-name="Multilingual App Toolkit" tool-version="4.0.1795.0" tool-company="Microsoft" />
    </header>
    <body>
      <group id="ResxGroup" datatype="resx">
        <trans-unit id="Password.Required" translate="yes" xml:space="preserve">
          <source>You must provide a valid password.</source>
          <target state="new">You must provide a valid password.</target>
        </trans-unit>
        <trans-unit id="Confirm.Required" translate="yes" xml:space="preserve">
          <source>You must provide a password confirmation.</source>
          <target state="new">You must provide a password confirmation.</target>
        </trans-unit>
        <trans-unit id="Confirm.Text" translate="yes" xml:space="preserve">
          <source>Confirm Password</source>
          <target state="new">Confirm Password</target>
        </trans-unit>
        <trans-unit id="cmdChangePassword.Text" translate="yes" xml:space="preserve">
          <source>Change Password</source>
          <target state="new">Change Password</target>
        </trans-unit>
        <trans-unit id="FailedAttempt.Text" translate="yes" xml:space="preserve">
          <source>Failed to reset password. Either the user name/token combination is invalid, or the token has expired.</source>
          <target state="new">Failed to reset password. Either the user name/token combination is invalid, or the token has expired.</target>
        </trans-unit>
        <trans-unit id="Password.Text" translate="yes" xml:space="preserve">
          <source>New Password</source>
          <target state="new">New Password</target>
        </trans-unit>
        <trans-unit id="SuccesfulAttempt.Text" translate="yes" xml:space="preserve">
          <source>The user account has had their password updated.</source>
          <target state="new">The user account has had their password updated.</target>
        </trans-unit>
        <trans-unit id="Username.Required" translate="yes" xml:space="preserve">
          <source>You must provide a username.</source>
          <target state="new">You must provide a username.</target>
        </trans-unit>
        <trans-unit id="Username.Text" translate="yes" xml:space="preserve">
          <source>Username</source>
          <target state="new">Username</target>
        </trans-unit>
        <trans-unit id="ControlTitle_passwordreset.Text" translate="yes" xml:space="preserve">
          <source>Password Reset</source>
          <target state="new">Password Reset</target>
        </trans-unit>
        <trans-unit id="Email.Required" translate="yes" xml:space="preserve">
          <source>You must provide your email address.</source>
          <target state="new">You must provide your email address.</target>
        </trans-unit>
        <trans-unit id="Email.Text" translate="yes" xml:space="preserve">
          <source>E-Mail Address</source>
          <target state="new">E-Mail Address</target>
        </trans-unit>
        <trans-unit id="ForcedResetInfo.Text" translate="yes" xml:space="preserve">
          <source>Your password needs to be changed before you can login into your account. A password reset link has been sent to your email address which will allow you to create a new password.</source>
          <target state="new">Your password needs to be changed before you can login into your account. A password reset link has been sent to your email address which will allow you to create a new password.</target>
        </trans-unit>
        <trans-unit id="ChangeSuccessful.Text" translate="yes" xml:space="preserve">
          <source>Password Changed Successfully</source>
          <target state="new">Password Changed Successfully</target>
        </trans-unit>
        <trans-unit id="Answer.Text" translate="yes" xml:space="preserve">
          <source>Answer</source>
          <target state="new">Answer</target>
        </trans-unit>
        <trans-unit id="Answer.Required" translate="yes" xml:space="preserve">
          <source>You must provide the security answer.</source>
          <target state="new">You must provide the security answer.</target>
        </trans-unit>
        <trans-unit id="ResetLinkExpired.Text" translate="yes" xml:space="preserve">
          <source>The password reset link has expired and is no longer valid.</source>
          <target state="new">The password reset link has expired and is no longer valid.</target>
        </trans-unit>
      </group>
    </body>
  </file>
